//Ejercicio numero 1.

let num1 = parseInt (prompt("ingrese el primer numero"));
let num2 = parseInt (prompt("ingrese el segundo numero"));
let resultado = num1 + num2;
console.log(resultado);

//Ejercicio numero 2.

let vel1, vel2, distancia, tiempo;
distancia = parseInt(prompt("distancia"));
vel1 = parseInt (prompt("vel1"));
vel2 = parseInt (prompt("vel2"));
tiempo = distancia / (vel1 - vel2);
console.log(tiempo);
tiempo = tiempo * 60;
console.log("El auto lo alcanza en" + tiempo + "minutos");