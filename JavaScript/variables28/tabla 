def tablemultiplicar (num):
    for i in range(1,11):
      multiplicacion = print(f"{num} x {i} = {num * i}")
    return multiplicacion

j=1 
while j==1:
    num = int(input("¿Qué tabla de multiplicar deseas realizar?:  "))
    tablemultiplicar(num)
    print("\n ")
    print("¿Desea realizar otra tabla de multiplicar?")
    print(" 1. Para realizar")
    print(" 0. Para salir")
    j=int(input("Digite su opción  "))

